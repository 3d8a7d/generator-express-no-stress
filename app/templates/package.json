{
  "name": "myapp",
  "version": "1.0.0",
  "description": "My cool app",
  "main": "index.js",
  "scripts": {
    "start": "cd build && node index.js",
    "compile": "babel server --out-dir build --delete-dir-on-start --source-maps inline --copy-files",
    "dev": "nodemon server/ --exec babel-node | pino -o 'short'",
    "debug": "nodemon server --exec babel-node --inspect | pino -o 'short'",
    "test": "_mocha --compilers js:@babel/register --exit"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "pino": "^4.17.6",
    "swagger-express-middleware": "^1.0.0-alpha.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^8.2.2",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.13.0",
    "mocha": "^5.0.5",
    "nodemon": "^1.18.4",
    "supertest": "^3.0.0"
  },
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)"
}
